APP_ENV=development
API_KEY=your-api-key
MAX_REQUESTS_PER_MINUTE=60
MEMORY_LOG_INTERVAL_SECONDS=300

# PostgreSQL Database Configuration
PG_DB_CONNECTION=postgres
PG_DB_HOST=localhost
PG_DB_PORT=5432
PG_DB_DATABASE=connectra
PG_DB_USERNAME=postgres
PG_DB_PASSWORD=password
PG_DB_DEBUG=false
PG_DB_SSL=false

# OpenSearch Configuration
OPEN_SEARCH_CONNECTION=http
OPEN_SEARCH_HOST=localhost
OPEN_SEARCH_PORT=9200
OPEN_SEARCH_USERNAME=admin
OPEN_SEARCH_PASSWORD=password
OPEN_SEARCH_DEBUG=false
OPEN_SEARCH_SSL=false
OPEN_SEARCH_AUTH=false

# Job Configuration
JOB_IN_QUEUE_SIZE=100
PARALLEL_JOBS=4
TICKER_INTERVAL_MINUTES=1
BATCH_SIZE_FOR_INSERTION=100
JOB_TYPE=insert_csv_file

# S3 Storage Configuration
S3_ACCESS_KEY=your-access-key
S3_SECRET_KEY=your-secret-key
S3_REGION=us-east-1
S3_BUCKET=your-bucket-name
S3_ENDPOINT=s3.amazonaws.com
S3_SSL=true
S3_DEBUG=false
S3_UPLOAD_URL_TTL_HOURS=24
S3_UPLOAD_FILE_PATH_PRIFIX=uploads
